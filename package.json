{
  "name": "mado-orchestrator",
  "version": "1.0.0",
  "description": "Multi-Agent Development Orchestrator - A sophisticated orchestrator-agent system using Claude Code and Git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "agent:orchestrator": "tsx src/cli/commands/start-agent.ts --role=orchestrator",
    "agent:frontend": "tsx src/cli/commands/start-agent.ts --role=frontend",
    "agent:backend": "tsx src/cli/commands/start-agent.ts --role=backend",
    "agent:devops": "tsx src/cli/commands/start-agent.ts --role=devops",
    "agent:qa": "tsx src/cli/commands/start-agent.ts --role=qa",
    "setup": "node scripts/setup.js",
    "demo": "node scripts/demo.js",
    "clean": "rimraf dist"
  },
  "keywords": [
    "ai",
    "agents",
    "orchestrator",
    "git",
    "claude-code",
    "development",
    "automation",
    "typescript"
  ],
  "author": "MADO Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "simple-git": "^3.19.1",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "axios": "^1.4.0",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "yaml": "^2.3.1",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "@types/express": "^4.17.17",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/ws": "^8.5.5",
    "@types/jest": "^29.5.3",
    "@types/node-cron": "^3.0.8",
    "typescript": "^5.1.6",
    "tsx": "^3.12.7",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "eslint": "^8.46.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/mado-orchestrator.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/mado-orchestrator/issues"
  },
  "homepage": "https://github.com/your-org/mado-orchestrator#readme"
}